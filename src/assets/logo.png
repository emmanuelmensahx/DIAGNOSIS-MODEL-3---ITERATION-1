<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background Circle -->
  <circle cx="100" cy="100" r="95" fill="#1E88E5" stroke="#FFFFFF" stroke-width="10"/>
  
  <!-- Medical Cross -->
  <rect x="85" y="60" width="30" height="80" fill="#FFFFFF" rx="5"/>
  <rect x="60" y="85" width="80" height="30" fill="#FFFFFF" rx="5"/>
  
  <!-- AI Circuit Pattern -->
  <g stroke="#E3F2FD" stroke-width="2" fill="none">
    <!-- Top circuits -->
    <circle cx="70" cy="50" r="3" fill="#E3F2FD"/>
    <circle cx="130" cy="50" r="3" fill="#E3F2FD"/>
    <line x1="70" y1="50" x2="85" y2="65"/>
    <line x1="130" y1="50" x2="115" y2="65"/>
    
    <!-- Bottom circuits -->
    <circle cx="70" cy="150" r="3" fill="#E3F2FD"/>
    <circle cx="130" cy="150" r="3" fill="#E3F2FD"/>
    <line x1="70" y1="150" x2="85" y2="135"/>
    <line x1="130" y1="150" x2="115" y2="135"/>
    
    <!-- Side circuits -->
    <circle cx="40" cy="100" r="3" fill="#E3F2FD"/>
    <circle cx="160" cy="100" r="3" fill="#E3F2FD"/>
    <line x1="40" y1="100" x2="60" y2="100"/>
    <line x1="160" y1="100" x2="140" y2="100"/>
  </g>
  
  <!-- AfriDiag Text -->
  <text x="100" y="180" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1E88E5">AfriDiag</text>
</svg>